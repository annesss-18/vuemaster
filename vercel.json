{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "next build",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "regions": ["iad1"],
  "functions": {
    "app/api/live/route.ts": {
      "maxDuration": 300,
      "memory": 1024,
      "runtime": "nodejs20.x"
    }
  },
  "headers": [
    {
      "source": "/api/live",
      "headers": [
        {
          "key": "Connection",
          "value": "Upgrade"
        },
        {
          "key": "Upgrade",
          "value": "websocket"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/live",
      "destination": "/api/live"
    }
  ]
}